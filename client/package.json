{
  "name": "yourstory-client",
  "version": "1.0.0",
  "description": "The yourstory.blog website client (front end)",
  "main": "src/index.js",
  "scripts": {
    "install-development": "npm install",
    "install-production": "npm install --only=production",
    "compile-webpack-config": "tsc --lib es6 --esModuleInterop webpack.config.ts",
    "build-development": "npm run compile-webpack-config && webpack",
    "build-production": "npm run compile-webpack-config && webpack --env production",
    "watch": "webpack --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vargaeric/yourstory.git"
  },
  "keywords": [
    "yourstory",
    "story",
    "blog",
    "client"
  ],
  "author": "Varga Erik Zsolt",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/vargaeric/yourstory/issues"
  },
  "homepage": "https://github.com/vargaeric/yourstory#readme",
  "dependencies": {
    "@emotion/react": "11.9.0",
    "@emotion/styled": "11.8.1",
    "@mui/icons-material": "5.6.2",
    "@mui/lab": "5.0.0-alpha.83",
    "@mui/material": "5.6.2",
    "@reduxjs/toolkit": "1.8.1",
    "@types/react": "18.0.12",
    "@types/react-dom": "18.0.5",
    "html-webpack-plugin": "5.5.0",
    "path": "0.12.7",
    "prop-types": "15.8.1",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-redux": "8.0.1",
    "react-router-dom": "6.3.0",
    "ts-loader": "9.3.0",
    "typescript": "4.7.3",
    "webpack": "5.73.0",
    "webpack-cli": "4.9.2"
  }
